#:import dp kivy.metrics.dp

<VitrineScreen@BaseScreen>:
    titulo: "Produtos"

    ScrollView:
        GridLayout:
            id: produtos_box
            cols: 3
            spacing: dp(15)
            padding: [dp(15), dp(10), dp(15), dp(10)]
            size_hint_y: None
            height: self.minimum_height